<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myshoppinglistapp">

    <application
        android:allowBackup="true"
        android:name=".InitApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.menu.MenuActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myshoppinglistapp.presentation.shoppingList.ShoppingListActivity"/>
        <activity android:name=".presentation.addShoppingList.AddShoppingListActivity" />
        <activity android:name=".presentation.archivedShoppingList.ArchivedShoppingListActivity" />
        <activity android:name=".presentation.archivedShoppingList.ArchiveShoppingList.ArchiveShoppingListActivity" />
    </application>

</manifest>